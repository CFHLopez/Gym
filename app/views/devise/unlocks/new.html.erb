<%= form_for(resource, as: resource_name, url: session_path(resource_name)), html: { method: :post }) do |f| %>
	<%= devise_error_messages! %>
  	<div class="container">
    	<h2><b>Reenviar Instrucciones De desbloqueo</h2>
    	<div class="panel-group"><big>  
      		<div class="panel-group">
        		<%= "Email" %><br></b>
        		<%= f.email_field :email, autofocus: true, placeholder: "ejemplo@ejemplo.cl" %>
      		</div>
      	<div class="actions">
        <%= f.submit "Reenviar", class: "btn btn-success" %>
      </div>
    </div>
  </div>
<% end %>